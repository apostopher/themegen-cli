{
  "extends": [
    "oclif",
    "oclif-typescript",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended",
    "eslint-config-prettier/@typescript-eslint",
    "plugin:import/typescript"
  ],
  "plugins": ["import"],
  "rules": {
    "import/order": ["error", { "newlines-between": "always", "alphabetize": { "order": "asc" } }]
  }
}
